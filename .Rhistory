library(readxl)
Bone_Tumor_Dataset_csv <- read_excel("Bone Tumor Dataset.csv.xlsx")
View(Bone_Tumor_Dataset_csv)
head(Bone_Tumor_Dataset_csv)
names(Bone_Tumor_Dataset_csv)
bone <- Bone_Tumor_Dataset_csv
SEX_COL <- "Sex"
STATUS_COL <- "Status (NED, AWD, D)"
bone2 <- bone[, c(SEX_COL, STATUS_COL)]
bone2[[SEX_COL]] <- factor(bone2[[SEX_COL]])
bone2 <- subset(bone2,
bone2[[SEX_COL]] != "" &
bone2[[STATUS_COL]] != "" &
!is.na(bone2[[SEX_COL]]) &
!is.na(bone2[[STATUS_COL]]))
sex_status_tab <- table(bone2[[SEX_COL]], bone2[[STATUS_COL]])
female_values <- sex_status_tab[1, ]
female_labels <- paste(names(female_values), " (", female_values, ")", sep = "")
pie(
female_values,
main = paste("Status Distribution -", rownames(sex_status_tab)[1]),
labels = female_labels,
col = rainbow(length(female_values))
)
